<?xml version="1.0"?>
<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:ak_google_avail/locallang_db.php:pi_flexform.sheet_general</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <mode>
            <TCEforms>
              <label>Modus</label>
              <config>
                <type>select</type>
                <default>0</default>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0"></numIndex>
                    <numIndex index="1"></numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">LLL:EXT:ak_google_avail/locallang_db.xml:pi_flexform.mode_single</numIndex>
                    <numIndex index="1">single</numIndex>
                  </numIndex>
                  <numIndex index="2" type="array">
                    <numIndex index="0">LLL:EXT:ak_google_avail/locallang_db.xml:pi_flexform.mode_multiple</numIndex>
                    <numIndex index="1">multiple</numIndex>
                  </numIndex>
                </items>
              </config>
              <onChange>reload</onChange>
            </TCEforms>
          </mode>
          <url>
            <TCEforms>
              <label>Kalender-URL (ICS)</label>
              <config>
                <type>input</type>
                <size>200</size>
                <max>4096</max>
                <eval>trim</eval>
              </config>
              <displayCond>FIELD:mode:=:single</displayCond>
            </TCEforms>
          </url>
          <names>
            <TCEforms>
              <label>Kalenderbezeichnungen (eine pro Zeile)</label>
              <config>
                <type>text</type>
                <cols>60</cols>
                <rows>10</rows>
              </config>
              <displayCond>FIELD:mode:=:multiple</displayCond>
            </TCEforms>
          </names>
          <pages>
            <TCEforms>
              <exclude>1</exclude>
              <label>Link-Zielseiten</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <minitems>0</minitems>
                <maxitems>100</maxitems>
                <size>10</size>
                <multiple>1</multiple>
              </config>
              <displayCond>FIELD:mode:=:multiple</displayCond>
            </TCEforms>
          </pages>
          <urls>
            <TCEforms>
              <label>Kalender-URLs (ICS, eine pro Zeile)</label>
              <config>
                <type>text</type>
                <cols>60</cols>
                <rows>10</rows>
              </config>
              <displayCond>FIELD:mode:=:multiple</displayCond>
            </TCEforms>
          </urls>
        </el>
